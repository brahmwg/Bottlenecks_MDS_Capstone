 SELECT dd.location,
    dd.datetime,
    dd.tagid,
    dd.tag_mfr,
    dd.tag_id,
    dd.antenna,
    dd.amplitude_mv,
    dd.loc_scanner,
    dd.datedownload,
    dd.date_delta,
    dd.fname,
    dd.dirname,
    dd.tag_size,
    dd.site_name,
    dd.nearest_town,
    dd.watershed,
    dd.site_description,
    dd.mc,
    dd.is1001,
    dd.subloc,
    dd.location_code,
    dd.latitude,
    dd.longitude,
    dd.utm_easting,
    dd.utm_northing,
    dd.antennas,
    dd.installation,
    dd.removal,
    dd.location_type,
    dd.sitenotes,
    dd.othernotes,
    dd.jamiesonnotes,
    dd.date,
    f.date AS field_date,
    f.year,
    f.month,
    f.watershed AS field_watershed,
    f.river,
    f.site,
    f.method,
    f.local,
    f.crew,
    f.pit_scanner,
    f.pit_tray,
    f.day_start_time,
    f.water_temp_start,
    f.day_comments,
    f.day_end_time,
    f.water_temp_end,
    f.species,
    f.fork_length_mm,
    f.weight_g,
    f.clip_status,
    f.tag_status,
    f.scanner_time,
    f.tag_id_long,
    f.whatman_sheet_ck,
    f.whatman_sheet_co,
    f.whatman_no,
    f.fish_comments,
    f.set_no,
    f.whatman_concat_ck,
    f.whatman_concat_co,
    f.whatman_concat_ck2,
    f.whatman_concat_co2
   FROM ods.field f
     JOIN ods.downstream_detections dd ON f.tag_id_long = dd.tagid
  WHERE f.tag_id_long IS NOT NULL;

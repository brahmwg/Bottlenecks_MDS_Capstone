 SELECT date,
    year,
    month,
    watershed,
    river,
    site,
    method,
    local,
    crew,
    pit_scanner,
    pit_tray,
    day_start_time,
    water_temp_start,
    day_comments,
    day_end_time,
    water_temp_end,
    species,
    fork_length_mm,
    weight_g,
    clip_status,
    tag_status,
    scanner_time,
    tag_id_long,
    whatman_sheet_ck,
    whatman_sheet_co,
    whatman_no,
    fish_comments,
    set_no,
    whatman_concat_ck,
    whatman_concat_co,
    whatman_concat_ck2,
    whatman_concat_co2,
    indiv,
    sample_id,
    lane,
    stockcode,
    catchyear,
    catchjuliandate,
    fish,
    vial,
    tray,
    samplename,
    lifestage,
    clipyn,
    comments,
    pid,
    extractiondateyyyymmdd,
    extractiontech,
    exportdateyyyymmdd,
    pcrtech,
    resultssentddmmyyyy,
    status,
    comments2,
    comments3,
    comments4,
    id_source,
        CASE
            WHEN clip_status = 'unclip'::text THEN
            CASE
                WHEN id_source = 'PBT'::text THEN 'hatchery'::text
                WHEN id_source = 'GSI'::text THEN 'wild'::text
                ELSE 'unknown'::text
            END
            WHEN clip_status = 'clip'::text THEN 'hatchery'::text
            ELSE 'unknown'::text
        END AS wild_or_hatchery
   FROM ods.field_genetics;

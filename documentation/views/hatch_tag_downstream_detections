 SELECT ht.tray,
    ht.crew,
    ht.system,
    ht.species,
    ht.cohort,
    ht.outmigration_y,
    ht.hex,
    ht.tag_id_long,
    ht.cwt,
    ht.date_time_release,
    ht.fork_length_mm,
    ht.origin,
    ht.clip_status,
    ht.sampler,
    ht.stage,
    ht.release_location,
    ht.release_latitude,
    ht.release_longitude,
    ht.river_km,
    ht.dna,
    ht.scale_book,
    ht.scale_num,
    ht.depth_m,
    dd.location,
    dd.datetime,
    dd.tagid,
    dd.tag_mfr,
    dd.tag_id,
    dd.antenna,
    dd.amplitude_mv,
    dd.loc_scanner,
    dd.datedownload,
    dd.date_delta,
    dd.fname,
    dd.dirname,
    dd.tag_size,
    dd.site_name,
    dd.nearest_town,
    dd.watershed,
    dd.site_description,
    dd.mc,
    dd.is1001,
    dd.subloc,
    dd.location_code,
    dd.latitude,
    dd.longitude,
    dd.utm_easting,
    dd.utm_northing,
    dd.antennas,
    dd.installation,
    dd.removal,
    dd.location_type,
    dd.sitenotes,
    dd.othernotes,
    dd.jamiesonnotes,
    dd.date
   FROM ods.hatch_tag ht
     JOIN ods.downstream_detections dd ON ht.tag_id_long = dd.tagid
  WHERE ht.tag_id_long IS NOT NULL;
